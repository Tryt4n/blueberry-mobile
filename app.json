{
  "expo": {
    "name": "Bor√≥wka",
    "slug": "blueberry-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/favicon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/favicon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "plugins": ["expo-router", "@react-native-google-signin/google-signin"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.plantation.blueberry",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.631863393828-ifq3irpfp6solcs7ftmuq9b1vshfrt7k"
            ]
          }
        ]
      },
      "icon": "./assets/images/favicon.png",
      "package": "com.plantation.blueberry"
    },
    "android": {
      "package": "com.plantation.blueberry",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/favicon.png",
        "backgroundColor": "transparent"
      },
      "android.permissions": ["VIBRATE"]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "81351758-6a3e-4c2a-81de-d9286a33091b"
      }
    }
  }
}
